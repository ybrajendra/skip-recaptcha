{
    "name": "cloudcommerce/skip-recaptcha",
    "description": "Magento 2 module to skip reCAPTCHA validation for whitelisted IP addresses",
    "type": "magento2-module",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "CloudCommerce",
            "email": "info@cloudcommerce.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "magento/framework": ">=103.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "CloudCommerce\\SkipRecaptcha\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}